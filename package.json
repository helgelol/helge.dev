	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"e2e": "playwright test",
		"clean": "rm -rf node_modules .svelte-kit build dist",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
